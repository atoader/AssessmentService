        <omdoc:term 
        stex:srcref="../../../fcad/flange/cds/price-function.tex#textrange(from=4;5,to=5;83)" about="#price-function.p1.p1.term2" xml:id="price-function.p1.p1.term2" name="offer" cd="financial-transaction">
          offer
</omdoc:term>
