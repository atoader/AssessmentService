      <p about="#price-function.p1.p1" class="ltx_p" id="price-function.p1.p1">
        The
        <omdoc:term 
        stex:srcref="../../../fcad/flange/cds/price-function.tex#textrange(from=4;48,to=5;40)" about="#price-function.p1.p1.term1" xml:id="price-function.p1.p1.term1" name="price" cd="financial-transaction">
          price
</omdoc:term>
