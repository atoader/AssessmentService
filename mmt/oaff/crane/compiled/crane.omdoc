<omdoc base="http://docs.omdoc.org/crane/crane.omdoc">
  <theory name="crane" base="http://docs.omdoc.org/crane/crane.omdoc">
    <import from="../../../fcad/crane/cds/sheave?sheave"> </import>
    <import from="../../../fcad/crane/cds/winch?winch"> </import>
    <import from="../../../fcad/crane/cds/hoist?hoist"> </import>
